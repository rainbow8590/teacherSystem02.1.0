/**index.wxss**/
page{
  height: 100%;
}
.container{
  padding: 0;
  justify-content: flex-start;
  color: #32343d;
  background: #fafafa;
}

.flex{
  display: flex;
}

/*搜索条件*/
.searchW{
  width: 100%;
  height: 108rpx;
  padding:30rpx 0 30rpx 28rpx;
  border-bottom: 1px solid #1FB923;
  box-sizing: border-box;
  background: #fff;
}
.searchW .active{
  background: #1FB923;
  color: #fff;
}

.searchW .menu{
  padding: 16rpx 0 37rpx 263rpx;
}
.searchW .menu image{
  display: block;
  width: 49rpx;
  height: 28rpx; 
}
.searchW .searchIf .searchIf-item{
  position: relative;
  width: 132rpx;
  height: 52rpx;
  line-height: 52rpx;
  padding-left: 30rpx;
  border-radius: 26rpx;
  background: #eee;
  font-size: 24rpx;
  margin-right: 30rpx;
}
.searchW .searchIf .searchIf-item image{
  position: absolute;
  right: 17rpx;
  top: 50%;
  margin-top: -5rpx;
  width: 20rpx;
  height: 12rpx;
}
.searchResult{
  width: 100%;
  height: 400px;
  padding-left: 28rpx;
  padding-bottom: 114rpx;
  box-sizing: border-box;
  background: #fff;
  overflow-y:scroll;
}
.searchResult .resultItem{
  width: 100%;
  height: 112rpx;
  line-height: 112rpx;
  font-size: 26rpx;
  border-bottom: 1px solid #ddd;
}
.searchResult .resultItem .dot{
  width: 10rpx;
  height: 10rpx;
  background: #b2b8bc;
  border-radius: 50%;
  margin: auto 0;
  margin-left: 10rpx;
  margin-right: 28rpx;
}

/*  导航   */
.tabBar{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 55px;
  border-top: 1px solid #DADADA;
  overflow:hidden;
  background:#fafafa;
}
.tabBar .tabItem{
  width: 33.3%;
  height: 100%;
  font-size: 15px;
  color: #525252;
  text-align: center;
  padding: 5px 0;
}
.tabBar .tabItem .icon{
  
  height: 24px;
  overflow: hidden;
}
.tabBar .tabItem image{
  margin: 0 auto;
  width: 26px;
  height: 24px;
}
.tabBar .tabItem .txt{
  height: 15px;
  line-height: 15px;
  padding-top: 6px;
}
/* 导航 */
.mainMenu{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.2);
  padding-left: 320rpx;
  box-sizing: border-box;
  z-index: 10000;
}
.mainMenu .menu{
  width: 430rpx;
  height: 100%;
  background: #fff;
}
.mainMenu .con{position: relative;}
