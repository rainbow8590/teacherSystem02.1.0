.mainMenu .con navigator {color:#666;display:block;text-indent:10px;font-size:28rpx;padding: 35rpx 0 35rpx 68rpx;}
.mainMenu .btn {background:#1FB923; margin: 0 10%;width: 80%;height: 52rpx;line-height:52rpx;font-size: 30rpx;color: #fff;border-radius: 26rpx;display: block; margin-top: 132rpx;padding-top: 0;}
.drawer_screen{background:rgba(0, 0, 0, .5);width:100%;height:100%;position:fixed;left:0;top:0;z-index:1000;}
.userinfo { height: 228rpx;padding:146rpx 0 0 0;background: #1FB923; margin-bottom: 60rpx; justify-content: center;align-items: center;}
.userinfo .Avatar {text-align: center;width: 85px;height: 85px;margin-right: 34rpx ;background: #5c7c97;border-radius: 50%;}
.userinfo .Avatar image {width:50px;margin-top: 15px;}
.userinfo .name {color: #fff;line-height: 24px; font-size: 16px;text-align: center}

/*弹窗*/
.float{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.3);
  z-index: 10000;
}
.float .floatWrap{
  width: 80%;
 /* height: 460rpx;*/
  padding: 0 12rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40%;
  margin-top: -280rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.float .floatWrap .arrows{
  position: relative;
  height: 67rpx;
}
.float .floatWrap .arrows image{
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -9rpx;
  margin-top: -5rpx;
  width: 18rpx;
  height: 11rpx;
}
.float .floatWrap .floatC{
  height: 426rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}
.float .floatWrap .floatC .floatC-item{
  height: 97rpx;
  line-height: 97rpx;
  padding-left:0;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
  color: #333;
}
.float1 .floatWrap .floatC .floatC-item{
  padding-left:40rpx;
  text-align: left;
}
.float1 .floatWrap .floatC .floatC-item label{
  display: block;
  width: 100%;
}

