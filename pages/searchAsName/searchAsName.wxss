/**index.wxss**/
page{
  height: 100%;
}
.container{
  padding: 0;
  background: #fafafa;
  position: relative;
}
.flex{
  display: flex;
}
/*  导航   */
.tabBar{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 109rpx;
  border-top: 1px solid #DADADA;
}
.tabBar .tabItem{
  width: 33.3%;
  height: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #525252;
}
.tabBar .tabItem image{
  width: 52rpx;
  height: 47rpx;
}


/*主体内容*/
.wrap{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
  box-sizing: border-box;
}

.wrap .classInfo{
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  padding: 0 30rpx 0 48rpx;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #1fba21;
  background: #fff;
  box-sizing: border-box;
}
.wrap .classInfo image{
  width: 49rpx;
  height: 28rpx;

}
.wrap .classInfo text{
  margin-left: 46rpx;
}
.wrap .choise{
  padding: 16rpx;
  padding-left: 46rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #1fba21;
  background: #fff;
  font-size: 30rpx;
  color: #1fba21;
}
.wrap .choise image{
  width: 16rpx;
  height: 18rpx;
  padding-right: 10rpx;
}
.wrap .table{
  overflow: hidden;
  background: #fff;
}
.wrap .table .thead{
  position: relative;
  height: 89rpx;
  line-height: 89rpx;
  border-bottom: 1px solid #1fba21;
  font-size: 26rpx;
  text-align: center;
  z-index: 1000;
}
.wrap .table .tbody{
  position: relative;
  overflow: hidden;
  overflow-y:scroll;
  border-bottom-color: transparent;
  z-index: 999;
}
.wrap .table .thead .thead-left{
  position: relative;
  background: #fff;
  z-index: 99;
}
.wrap .table .tbody .thead-left .leftItem{
  border-bottom: 1px solid #ddd;
}
.wrap .table .tbody .thead-right .scrollItem{
  border-bottom: 1px solid #ddd;
}
.wrap .table .thead .thead-left .leftItem{
  background: #fff;
}
.wrap .table .thead .thead-right{
  position: relative;
  background: #fff;
  z-index: 88;
}
.wrap .table .thead .thead-right .rightWrap{
  position: absolute;
  left: 0;
  top: 0;
}
.wrap .table .thead .twrap{
  position: absolute;
  left: 0;
  top: 0;
}
.wrap .table  .sName{
  width: 80px;
  text-align: center;
}
.wrap .table .sKe{
  width: 80px;
  text-align: center;
  position: relative;
}
.wrap .table .sKe .bubble{
  position: absolute;
  left: 50%;
  bottom: 86rpx;
  margin-left: -80%;
  background: #d0eab5;
  line-height: 30rpx;
  width: 160%;
  padding: 10rpx;
  border-radius: 20rpx;
  z-index:9999;
}
.wrap .table .sKe .bubble1{
  position: absolute;
  left: 50%;
  top: 84rpx;
  margin-left: -90%;
  background: #d0eab5;
  line-height: 30rpx;
  width: 160%;
  padding: 10rpx;
  border-radius: 20rpx;
  z-index:9999;
}
.wrap .table .sKe .bubble1 image{
  position: absolute;
  left: 50%;
  margin-left: -26rpx;
  top: -26rpx;
  width: 40rpx;
  height: 28rpx;
}
.wrap .table .sKe .bubble image{
  position: absolute;
  left: 50%;
  bottom: -26rpx;
  width: 40rpx;
  height: 28rpx;
}
.wrap .table .sSchool{
  width: 160px;
  padding: 0 10px;
  white-space: nowrap;
  overflow: hidden;
}
.wrap .table .sExperimental{
  width: 60px;
}
.wrap .table .sKejie{
  width: 60px;
}

/*弹窗*/
.float{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.3);
  z-index: 10000;
}
.float .floatWrap{
  width: 80%;
  height: 560rpx;
  padding: 0 12rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40%;
  margin-top: -280rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  overflow:hidden;
}

.float .floatWrap .floatC{
  height: 94%;
  margin: 20rpx 10rpx;
  line-height: 36rpx;
  overflow-y: scroll;
  overflow-x: hidden;
  font-size: 26rpx;
  color: #666;

}


