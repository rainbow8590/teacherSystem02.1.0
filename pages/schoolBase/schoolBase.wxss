/**index.wxss**/
page{
  height: 100%;
}
.container{
  padding: 0;
}
.flex{
  display: flex;
}

/*  导航   */
.tabBar{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 55px;
  border-top: 1px solid #DADADA;
  overflow:hidden;
}
.tabBar .tabItem{
  width: 33.3%;
  height: 100%;
  font-size: 15px;
  color: #525252;
  text-align: center;
  padding: 5px 0;
}
.tabBar .tabItem .icon{
  height: 24px;
  overflow: hidden;
}
.tabBar .tabItem image{
  margin: 0 auto;
  width: 26px;
  height: 24px;
}
.tabBar .tabItem .txt{
  height: 15px;
  line-height: 15px;
  padding-top: 6px;
}



.hoverLogin{
  background: #009502!important;
}
/*主体内容*/
.wrap{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fafafa;
  box-sizing: border-box;
}

.wrap .classInfo{
  width: 100%;
  height: 45px;
  line-height: 45px;
  padding: 0 30rpx 0 48rpx;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #1fba21;
  background: #fff;
  box-sizing: border-box;
}
.wrap .classInfo image{
  width: 49rpx;
  height: 28rpx;
}
.wrap .choise{
  height: 42px;
  padding: 16rpx;
  padding-left: 28rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #1fba21;
  background: #fff;
}
.wrap .choise .choiseItem{
  position: relative;
  padding-left: 30rpx;
  padding-right: 60rpx;
  margin-right: 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #eee;
  border-radius: 25rpx;
  font-size: 24rpx;
  color: #666;
}
.wrap .choise .choiseItem image{
  position: absolute;
  right: 20rpx;
  top: 50%;
  margin-top: -8rpx;
  width: 25rpx;
  height: 18rpx;
}
.wrap form{
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding-top: 14px;
  background: #fff;
}
.wrap form .thead{
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #1fba21;
  font-size: 26rpx;
  color: #666;
  margin: 0 20rpx;
}
.wrap form .thead .sName{
  width: 100rpx;
  padding-left: 14rpx;
  padding-right: 34rpx;
  text-align: center;
}
.wrap form .thead .sSchool{
  flex: 1;
  text-align: center;
}
.wrap form .thead .sExperimental{
  margin: 0 42rpx;
  width: 130rpx;
  text-align: center;
}
.wrap form .tbody {
  padding: 0 20rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  overflow-y: scroll;
}
.wrap form .tbody .tbody-row{
  height: 40rpx;
  line-height: 40rpx;
  padding: 20rpx 0;
  border-bottom: 1px solid #ddd;
  font-size: 23rpx;
  color: #666;
  
}
.wrap form .tbody .tbody-row .hidInp{
  width: 0;
  height: 0;
}
.wrap form .tbody .tbody-row .sName{
  width: 100rpx;
  padding-left: 14rpx;
  padding-right: 34rpx;
  text-align: center;
}
.wrap form .tbody .tbody-row .sSchool{
  flex: 1;
  text-align: center;
  height: 40rpx;
  border-radius: 20rpx;
  background: #F2F2F2;
  white-space: nowrap;
  padding: 0 20rpx;
  overflow:hidden;
   box-sizing: border-box;
}  

.wrap form .tbody .tbody-row .sSchool text{
  width: 100%;
  display: block;
  overflow: hidden;
}
.wrap form .tbody .tbody-row .sExperimental{
  position: relative;
  margin: 0 42rpx;
  width: 110rpx;
  padding-left: 20rpx;
  height: 40rpx;
  border-radius: 20rpx;
  text-align: left;
  background: #F2F2F2;
}
.wrap form .tbody .tbody-row .sExperimental image{
  position: absolute;
  right: 20rpx;
  top: 50%;
  margin-top: -5rpx;
  width: 17rpx;
  height: 10rpx;
}


/*期中期末*/


.wrap .scoreF .thead .sNumber{
  width: 100rpx;
  text-align: center;
}
.wrap .scoreF .thead .sSchool{
  flex: 1;
  text-align: center;
}
.wrap .scoreF .thead .sExperimental{
  margin: 0 42rpx;
  width: 130rpx;
  text-align: center;
}
.wrap .scoreF .tbody .tbody-row .sNumber{
  width: 100rpx;
  text-align: center;
} 

.wrap .scoreF .tbody .tbody-row .sExperimental{
  text-align: center;
}





.btn {
  padding-top: 15px;
  background: #fafafa;
}
.btn .loginBtn{
  width: 690rpx;
  height: 45px;
  background: #1fba21;
  color: #fff;
}

/*弹窗*/
.float{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.3);
  z-index: 10000;
}
.float .floatWrap{
  width: 80%;
  height: 560rpx;
  padding: 0 12rpx;
  background: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40%;
  margin-top: -280rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.float .floatWrap1{
  height: 690rpx;
  margin-top: -345rpx;
}
.float .floatWrap2{
  height: 300rpx;
  margin-top: -150rpx;

}
.float .floatWrap2 button{
  width: 80%;
  height: 60rpx;
  line-height: 60rpx;
  background: #ccc;
  margin-top: 60rpx;
}
.float .floatWrap .arrows{
  position: relative;
  height: 67rpx;
}
.float .floatWrap .arrows image{
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -9rpx;
  margin-top: -5rpx;
  width: 18rpx;
  height: 11rpx;
}
.float .floatWrap .floatC{
  height: 426rpx;

  overflow-y: scroll;
  overflow-X: hidden;
}
.float .floatWrap .floatC-item{
  width: 100%;
  height: 97rpx;
  line-height: 97rpx;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
  color: #333;
}
.float .floatWrap .floatC-item1{
  padding-top: 23rpx;
  box-sizing: border-box;
}
.float .floatWrap .floatC-item input{
  width: 460rpx;
  padding-left: 16rpx;
  background: #f1f1f1;
  margin: 0 auto;
  border-radius: 10rpx;
  text-align: left;
}


